.col-md-4
  form#formStart
    .card.card-tasks
      .card-header
        .d-flex.justify-content-between
          h4.card-title 퇴근시간
          button.btn.btn-primary(type='button' onclick='fnCreate("end_time", "#formStart")') 저장
      .card-body.overflow-auto(style='max-height:300px;')
        .table-full-width.table-responsive
          table.table
            thead.text-primary
              tr
                th.text-center #
                th.text-center 퇴근시간
                th.text-center
            tbody#new_end
              each r, i in end_time
                tr
                  td.text-center #{i + 1}
                  td.text-center
                    .form-group.my-0
                      input.form-control(type='text', value=r.end_time)
                  td.td-actions.text-right
                    button.btn.btn-info.btn-round.btn-icon.btn-icon-mini.btn-neutral(type='button', onclick='fnUpdate("' + r._id + '", this, "end_time")')
                      i.now-ui-icons.ui-2_settings-90
                    button.btn.btn-danger.btn-round.btn-icon.btn-icon-mini.btn-neutral(type='button', onclick='fnDelete("' + r._id + '", this, "end_time")')
                      i.now-ui-icons.ui-1_simple-remove
              tr
                td.text-center
                td.text-center
                  .form-group.my-0
                    input.form-control(type='text', name='end_time')
                td.text-center
                  a.btn.btn-info.btn-sm.px-2(onclick='fnNewRow("#new_end", "end_time")')
                    i.now-ui-icons.ui-1_simple-add.text-white(style='font-size:9px;')